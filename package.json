{
    "name": "grunt",
    "description": "The JavaScript Task Runner",
    "version": "1.1.0",
    "author": "Grunt Development Team (https://gruntjs.com/development-team)",
    "homepage": "https://gruntjs.com/",
    "repository": "https://github.com/gruntjs/grunt.git",
    "license": "MIT",
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "test": "node bin/grunt test",
        "test-tap": "node bin/grunt test:tap",
        "compile-libgrunt": "babel lib\\grunt --out-dir lib\\grunt --require babel-polyfill --presets env",
        "compile-testfixtures": "babel test\\fixtures --out-dir test\\fixtures --require babel-polyfill --presets env",
        "compile-testfixturesfilesdist": "babel test\\fixtures\\files\\dist --out-dir test\\fixtures\\files\\dist --require babel-polyfill --presets env",
        "compile-testfixturesfilessrc": "babel test\\fixtures\\files\\src --out-dir test\\fixtures\\files\\src --require babel-polyfill --presets env",
        "compile-testgrunt": "babel test\\grunt --out-dir test\\grunt --require babel-polyfill --presets env",
        "compile-testgruntfile": "babel test\\gruntfile --out-dir test\\gruntfile --require babel-polyfill --presets env",
        "compile-lib": "babel lib --out-dir lib --require babel-polyfill --presets env",
        "compile-testutil": "babel test\\util --out-dir test\\util --require babel-polyfill --presets env",
        "compile-libutil": "babel lib\\util --out-dir lib\\util --require babel-polyfill --presets env",
        "compile-testfixturesexpandjs": "babel test\\fixtures\\expand\\js --out-dir test\\fixtures\\expand\\js --require babel-polyfill --presets env",
        "compile-1_script": "npm run compile-libgrunt && npm run compile-testfixtures && npm run compile-testfixturesfilesdist && npm run compile-testfixturesfilessrc && npm run compile-testgrunt && npm run compile-testgruntfile && npm run compile-lib && npm run compile-testutil && npm run compile-libutil && npm run compile-testfixturesexpandjs",
        "compile": "npm run compile-1_script"
    },
    "main": "lib/grunt",
    "bin": {
        "grunt": "bin/grunt"
    },
    "keywords": [
        "task",
        "async",
        "cli",
        "minify",
        "uglify",
        "build",
        "lodash",
        "unit",
        "test",
        "qunit",
        "nodeunit",
        "server",
        "init",
        "scaffold",
        "make",
        "jake",
        "tool"
    ],
    "dependencies": {
        "coffeescript": "~1.10.0",
        "dateformat": "~1.0.12",
        "eventemitter2": "~0.4.13",
        "exit": "~0.1.1",
        "findup-sync": "~0.3.0",
        "glob": "~7.0.0",
        "grunt-cli": "~1.2.0",
        "grunt-known-options": "~1.1.0",
        "grunt-legacy-log": "~2.0.0",
        "grunt-legacy-util": "~1.1.1",
        "iconv-lite": "~0.4.13",
        "js-yaml": "~3.13.1",
        "minimatch": "~3.0.2",
        "mkdirp": "~1.0.3",
        "nopt": "~3.0.6",
        "path-is-absolute": "~1.0.0",
        "rimraf": "~2.6.2"
    },
    "devDependencies": {
        "difflet": "~1.0.1",
        "eslint-config-grunt": "~1.0.1",
        "grunt-contrib-nodeunit": "~2.0.0",
        "grunt-contrib-watch": "~1.1.0",
        "grunt-eslint": "~18.1.0",
        "temporary": "~0.0.4",
        "through2": "~2.0.0",
        "babel-cli": "latest",
        "babel-core": "latest",
        "babel-plugin-add-module-exports": "latest",
        "babel-plugin-transform-es2015-modules-commonjs": "latest",
        "babel-polyfill": "latest",
        "babel-plugin-transform-object-rest-spread": "latest",
        "babel-preset-env": "latest"
    },
    "files": [
        "lib",
        "bin"
    ]
}